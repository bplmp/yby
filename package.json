{
	"name": "mapascoletivos",
	"description": "A collaborative mapping platform",
	"version": "0.0.1",
	"author": {
		"name": "Ecolab"
	},
	"engines": {
		"node": "0.10.x",
		"npm": "1.2.x"
	},
	"main": "web.js",
	"scripts": {
		"watch": "watchify client/app.js -o public/app.js -v & ( sleep 3 && NODE_PATH=./app/controllers NODE_ENV=development ./node_modules/.bin/nodemon web.js)",
		"start": "NODE_PATH=./app/controllers NODE_ENV=development; browserify client/app.js | uglifyjs -mt > public/app.js & node web.js",
		"install": "napa",
		"test": "make test"
	},
	"dependencies": {
		"express": "latest",
		"express-namespace": "latest",
		"mongoose": "latest",
		"mongoose-user": "latest",
		"mongoose-schema-extend": "latest",
		"crypto": "latest",
		"passport": "latest",
		"passport-local": "latest",
		"passport-facebook": "latest",
		"passport-twitter": "latest",
		"passport-github": "latest",
		"passport-google-oauth": "latest",
		"passport-linkedin": "latest",
		"connect-mongo": "latest",
		"connect-flash": "latest",
		"view-helpers": "latest",
		"jade": "latest",
		"async": "latest",
		"underscore": "latest",
		"forever": "latest",
		"less": "latest",
		"less-middleware": "latest",
		"lorem-ipsum": "latest",
		"imager": "latest",
		"leaflet": "latest",
		"mapbox.js": "latest",
		"jquery": "latest",
		"jquery-ui-browserify": "latest",
		"markdown": "latest",
		"grvtr": "latest"
	},
	"devDependencies": {
		"watchify": "latest",
		"browserify": "latest",
		"uglify-js": "latest",
		"napa": "latest",
		"nodemon": "latest",
		"mocha": "latest",
		"should": "latest",
		"supertest": "latest",
		"factory-lady": "latest"
	},
	"napa": {
		"angular": "angular/bower-angular",
		"angular-route": "angular/bower-angular-route",
		"angular-sanitize": "angular/bower-angular-sanitize",
		"angular-resource": "angular/bower-angular-resource",
		"angular-animate": "angular/bower-angular-animate",
		"angular-elastic": "monospaced/angular-elastic",
		"angular-fitvids": "jimmyhillis/angular-fitvids",
		"angular-ui-utils": "angular-ui/ui-utils",
		"angular-ui-router": "angular-ui/ui-router",
		"angular-ui-sortable": "angular-ui/ui-sortable",
		"angular-modal": "miguelpeixe/angular-modal",
		"eventable": "madebymany/eventable",
		"sir-trevor": "madebymany/sir-trevor-js"
	},
	"keywords": [
		"mapas",
		"mapascoletivos",
		"openstreetmap"
	]
}
